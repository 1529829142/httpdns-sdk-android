<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/buttonAsync"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="124dp"
        android:text="异步解析"
        app:layout_constraintStart_toStartOf="@+id/textInput"
        app:layout_constraintTop_toBottomOf="@+id/textInput" />

    <Button
        android:id="@+id/buttonAsyncBatch"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="124dp"
        android:text="异步批量"
        app:layout_constraintEnd_toEndOf="@+id/textInput"
        app:layout_constraintTop_toBottomOf="@+id/textInput" />

    <Button
        android:id="@+id/button"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="进行解析"
        app:layout_constraintStart_toStartOf="@+id/textInput"
        app:layout_constraintTop_toBottomOf="@+id/textInput" />

    <Button
        android:id="@+id/buttonBatch"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="批量解析"
        app:layout_constraintEnd_toEndOf="@+id/textInput"
        app:layout_constraintTop_toBottomOf="@+id/textInput" />

    <EditText
        android:id="@+id/textInput"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:textSize="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        android:hint="输入待解析的域名，如：www.baidu.com"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/resultText"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="228dp"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/textInput"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textInput"
        app:layout_constraintTop_toBottomOf="@+id/textInput" />
</androidx.constraintlayout.widget.ConstraintLayout>